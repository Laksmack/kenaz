{
  "name": "@futhark/laguz",
  "version": "1.7.2",
  "description": "Laguz ᛚ — Notes and vault browser",
  "main": "dist/main/main/main.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "tsc -p tsconfig.main.json && NODE_ENV=development electron .",
    "dev:renderer": "vite",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "build": "npm run build:main && npm run build:renderer",
    "start": "electron .",
    "dist": "npm run build && electron-builder --mac",
    "dist:dir": "npm run build && electron-builder --mac --dir"
  },
  "build": {
    "appId": "com.futhark.laguz",
    "productName": "Laguz",
    "electronVersion": "39.6.0",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "!node_modules/**/*.map",
      "!node_modules/**/*.md"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "branding/icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "arm64"
          ]
        }
      ],
      "darkModeSupport": true,
      "identity": "MARTIN STENKILDE (DDZS7WM362)",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "../../signing/entitlements.mac.plist",
      "entitlementsInherit": "../../signing/entitlements.mac.plist",
      "forceCodeSigning": true
    },
    "publish": {
      "provider": "generic",
      "url": "https://kenaz.app/releases/laguz"
    },
